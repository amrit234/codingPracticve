
//Question 1
import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner Scanner = new Scanner(System.in);
		int t = Scanner.nextInt();
		while(t-->0){
		    int x = Scanner.nextInt();
		    int y = 2*x;
		    System.out.println(y*5);
		}
	}
}

//Question 2
/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner Scanner = new Scanner(System.in);
		int t = Scanner.nextInt();
		while(t-->0){
		    int x = Scanner.nextInt();
		    int y = Scanner.nextInt();
		    int z = Scanner.nextInt();
		    
		    int a = 5*x + 10*y;
		    int v = a/z;
		    System.out.println(v);
		}
	}
}

//Question 3
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner Scanner = new Scanner(System.in);
		int t = Scanner.nextInt();
		while(t-->0){
		    int x = Scanner.nextInt();
		    int y = Scanner.nextInt();
		    if(y<=x){
		        System.out.println(y*1);
		    }
		    else{
		        int a = (y-x)*2;
		        int b = x + a;
		        System.out.println(b);
		    }
		}
	}
}

// Question 4
/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner Scanner = new Scanner(System.in);
		int t = Scanner.nextInt();
		while(t-->0){
		  int x = Scanner.nextInt();
		  int y = Scanner.nextInt();
		  int z = Scanner.nextInt();
		  
		  if((x!=y)&&(x!=z)&&(y!=z)){
		      System.out.println("YES");
		  }
		  else{
		      System.out.println("NO");
		  }
		}
	}
}

//Question 5
/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner Scanner = new Scanner(System.in);
		int t = Scanner.nextInt();
		while(t-->0){
		    int x = Scanner.nextInt();
		    if((x%2==0)&&(x%7==0)){
		        System.out.println("Alice");
		    }
		    else if ((x%2!=0)&&(x%9==0)){
		        System.out.println("Bob");
		    } 
		    else{
		        System.out.println("Charlie");
		    }
		}
	}
}


